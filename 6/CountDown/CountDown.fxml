<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" prefWidth="260.0" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jp.ac.meijo_u.prog4.countdown.CountDownController">
   <children>
      <TextField fx:id="textCount" />
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Button fx:id="buttonStart" mnemonicParsing="false" onAction="#handleButtonStartAction" text="Start" />
         </children>
      </HBox>
      <Label fx:id="labelCount" text="Count Down">
         <font>
            <Font name="Lucida Console" size="24.0" />
         </font></Label>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
